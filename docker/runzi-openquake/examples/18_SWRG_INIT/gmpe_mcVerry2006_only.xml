<?xml version="1.0" encoding="UTF-8"?>

<nrml xmlns:gml="http://www.opengis.net/gml"
      xmlns="http://openquake.org/xmlns/nrml/0.4">
    <logicTree logicTreeID='lt1'>

            <logicTreeBranchSet uncertaintyType="gmpeModel" branchSetID="bs1"
                    applyToTectonicRegionType="Active Shallow Crust">
                <logicTreeBranch branchID="ASCr">
                    <uncertaintyModel>McVerry2006Asc</uncertaintyModel>
                    <uncertaintyWeight>1.0</uncertaintyWeight>
                </logicTreeBranch>
            </logicTreeBranchSet>

            <logicTreeBranchSet uncertaintyType="gmpeModel" branchSetID="bs2"
                    applyToTectonicRegionType="Subduction Intraslab">
                <logicTreeBranch branchID="Slab">
                    <uncertaintyModel>McVerry2006SSlab</uncertaintyModel>
                    <uncertaintyWeight>1.0</uncertaintyWeight>
                </logicTreeBranch>
            </logicTreeBranchSet>

        <logicTreeBranchSet uncertaintyType="gmpeModel" branchSetID="bs3"
                    applyToTectonicRegionType="Subduction Interface">
                <logicTreeBranch branchID="IntF">
                    <uncertaintyModel>McVerry2006SInter</uncertaintyModel>
                    <uncertaintyWeight>1.0</uncertaintyWeight>
                </logicTreeBranch>
            </logicTreeBranchSet>

        <logicTreeBranchSet uncertaintyType="gmpeModel" branchSetID="bs4"
                    applyToTectonicRegionType="Volcanic">
                <logicTreeBranch branchID="Volc">
                    <uncertaintyModel>McVerry2006Volc</uncertaintyModel>
                    <uncertaintyWeight>1.0</uncertaintyWeight>
                </logicTreeBranch>
            </logicTreeBranchSet>

    </logicTree>
</nrml>








